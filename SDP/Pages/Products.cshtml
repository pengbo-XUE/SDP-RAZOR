@page
@model SDP.Pages.ProductsModel
<!--Product Display ===============================================-->
<div class="container product_con">
    <div class="title">

        <h1>SD Perfumery</h1>
        <div class="sort">
            <select onchange="sortBy(this);">
                <option>Sort By</option>
                <option value="priceH-L">High-Low</option>
                <option value="priceL-H">Low-High</option>
                <option value="nameA-Z">A-Z </option>
                <option value="nameZ-A">Z-A</option>
            </select>
        </div>
    </div>
    <form method="post" asp-page="Products">
        <div class="row rowone flex-box" id="rowone"></div>
    </form>

</div>
<script>
    var productDbJson = @Html.Raw(Json.Serialize(MockDB.MockProductDB));
    console.log(productDbJson)
</script>
<script src="~/js/product.js" asp-append-version="true"></script>
<script src="~/js/sort.js" asp-append-version="true"></script>
